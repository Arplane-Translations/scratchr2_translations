msgstr "<img src=\"%(STATIC_URL)s/images/help/it/mod.png\" />"

#: templates/info/help/studio/tips/blocks/mod.html:7
msgid "Reports remainder from division of first number by second number"
msgstr ""
"Fornisce il resto della divisione del primo numero per il secondo numero"

#: templates/info/help/studio/tips/blocks/mod.html:11
#, python-format
msgid "<img src=\"%(STATIC_URL)s/images/help/mod-ex.png\" />"
msgstr "<img src=\"%(STATIC_URL)s/images/help/it/mod-ex.png\" />"

#: templates/info/help/studio/tips/blocks/mod.html:15
#, python-format
msgid ""
"<img src=\"%(STATIC_URL)s/images/help/mod.png\" /> gives the remainder of "
"the division of the \n"
"two numbers. For example, <img src=\"%(STATIC_URL)s/images/help/mod-ex2.png"
"\" /> is 5 (143 \n"
"divided by 6 is 23, and the remainder is 5).\n"
msgstr ""
"<img src=\"%(STATIC_URL)s/images/help/it/mod.png\" /> fornisce il resto della "
"divisione dei \n"
"deu numeri. Ad esempio, <img src=\"%(STATIC_URL)s/images/help/it/mod-ex2.png\" "
"/> restituisce il valore 5 (143 \n"
"diviso per 6 è 23, e il resto è 5).\n"

