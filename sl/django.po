msgstr "<img src=\"%(STATIC_URL)s/images/help/make-a-variable.png\" />"

#: templates/info/help/studio/tips/blocks/make-a-variable.html:7
msgid "Click to create and name a new variable"
msgstr "S klikom ustvari in poimenuj novo spremenljivko"

#: templates/info/help/studio/tips/blocks/make-a-variable.html:12
#, python-format
msgid ""
"\n"
"<img src=\"%(STATIC_URL)s/images/help/variable-ex.png\" />\n"
msgstr ""
"\n"
"<img src=\"%(STATIC_URL)s/images/help/variable-ex.png\" />\n"

#: templates/info/help/studio/tips/blocks/make-a-variable.html:18
#, python-format
msgid ""
